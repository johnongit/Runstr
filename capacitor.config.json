{
  "appId": "com.runstr.app",
  "appName": "Runstr",
  "webDir": "dist",
  "plugins": {
    "BackgroundGeolocation": {
      "locationProvider": 1,
      "desiredAccuracy": 10,
      "stationaryRadius": 50,
      "distanceFilter": 30,
      "debug": true,
      "stopOnTerminate": false,
      "startForeground": true,
      "foregroundServiceType": "location"
    }
  },
  "android": {
    "useLegacyBridge": true
  }
}
